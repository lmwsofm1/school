<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.school.mapper.UserMapper">

<!--            查询所有方法-->
        <select id="list" resultType="org.example.school.domain.entity.User">
            SELECT UserId, username, nick, email,  createTime,  updateTime FROM sys_user
        </select>
        <!--登录-->
    <select id="findByUserNameAndPassWord" resultMap="UserResultMap">
        SELECT * FROM users WHERE username = #{userName} AND password = #{passWord}
    </select>


</mapper>
